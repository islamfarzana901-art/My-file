#include <iostream>

using namespace std;

int main() {
    char X;
    cin >> X;

    // Check if the character is lowercase
    if (X >= 'a' && X <= 'z') {
        // Convert to uppercase by subtracting 32
        X = X - 32;
    } else if (X >= 'A' && X <= 'Z') {
        // Convert to lowercase by adding 32
        X = X + 32;
    }

    // Print the result
    cout << X << endl;

    return 0;
}
